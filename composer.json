    "require": {
        "magento/magento-composer-installer": "*"
    },
    "type": "magento2-module",
    "version": "2.0.0",
    "license": "OSL-3.0",
    "autoload": {
        "psr-4": {
            "Genmato\\": "app/code/Genmato/"
        }
    },
    "extra": {
        "map": [
            [
                "app/code/Genmato/MultiStoreSearchFields/*",
